      <Label layoutX="14.0" layoutY="14.0" prefHeight="17.0" prefWidth="100.0" text="Edit Customer" />
      <Label layoutX="300.0" layoutY="34.0" prefHeight="17.0" prefWidth="100.0" text="First Name" />
      <TextField fx:id="firstNameInput" layoutX="300.0" layoutY="51.0" prefHeight="25.0" prefWidth="291.0" />
      <Label layoutX="300.0" layoutY="90.0" prefHeight="17.0" prefWidth="100.0" text="Last Name" />
      <TextField fx:id="lastNameInput" layoutX="300.0" layoutY="107.0" prefHeight="25.0" prefWidth="291.0" />
      <Button layoutX="300.0" layoutY="146.0" mnemonicParsing="false" onAction="#saveClicked" text="Save" />
      <Button layoutX="350.0" layoutY="146.0" mnemonicParsing="false" onAction="#cancelClicked" text="Cancel" />
   </children>
</AnchorPane>
